<?xml version='1.0' encoding='utf-8'?>
<interface version="1.4">
  <project>
    <settings />
    <customwidgets />
  </project>
  <object class="tk.Toplevel" id="defaultWindow" named="True">
    <property name="geometry">480x680</property>
    <property name="minsize">480|680</property>
    <property name="title" translatable="yes">Modular Digital Methodologies Toolkit</property>
    <child>
      <object class="ttk.Button" id="button_OCR" named="True">
        <property name="command" type="command" cbtype="simple">on_ocr_item_clicked</property>
        <property name="text" translatable="yes">OCR</property>
        <layout manager="place">
          <property name="anchor">center</property>
          <property name="relheight">0.08</property>
          <property name="relwidth">0.75</property>
          <property name="relx">0.5</property>
          <property name="rely">0.1</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="button_bib_database" named="True">
        <property name="text" translatable="yes">Bibliometric Database</property>
        <layout manager="place">
          <property name="anchor">center</property>
          <property name="relheight">0.08</property>
          <property name="relwidth">0.75</property>
          <property name="relx">0.5</property>
          <property name="rely">0.2</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="button_bib_analysis" named="True">
        <property name="text" translatable="yes">Bibliometric Analysis</property>
        <layout manager="place">
          <property name="anchor">center</property>
          <property name="relheight">0.08</property>
          <property name="relwidth">0.75</property>
          <property name="relx">0.5</property>
          <property name="rely">0.3</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="button_pro_net_analysis" named="True">
        <property name="text" translatable="yes">Programming and Network Analysis</property>
        <layout manager="place">
          <property name="anchor">center</property>
          <property name="relheight">0.08</property>
          <property name="relwidth">0.75</property>
          <property name="relx">0.5</property>
          <property name="rely">0.4</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
